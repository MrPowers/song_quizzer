<!DOCTYPE html>
<html>
  <head>
    <% include partials/head %>
  </head>
  <body>
    <% include partials/navbar %>
    <div class="container">

      <div class="col-md-6">
        <h2 class="text-center">Spanish</h2>
        <% spanishSongs.forEach(function(song) { %>
          <a href="/spanish-songs/<%= song.artist %>/<%= song.name %>">
            <img src="/images/<%= song.image %>" alt="<%= song.artist %> <%= song.name %>" />
          </a>
        <% }); %>
      </div>

      <div class="col-md-6">
        <h2 class="text-center">English</h2>
        <% englishSongs.forEach(function(song) { %>
          <a href="/english-songs/<%= song.artist %>/<%= song.name %>">
            <img src="/images/<%= song.image %>" alt="<%= song.artist %> <%= song.name %>" />
          </a>
        <% }); %>
      </div>

    </div> <!--container-->
  </body>
</html>
