<!DOCTYPE html>
<html>
  <head>
    <title>Song Quizzer</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= language %> Songs</h1>
    <ul>
      <% songs.forEach(function(song) { %>
        <li><%= song.artist %> - <%= song.name %> <%= song.youtube_id %></li>
      <% }); %>
    </ul>
  </body>
</html>

